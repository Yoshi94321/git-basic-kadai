        "# 台形の寸法\n",
        "top_edge = 10  # cm\n",
        "bottom_edge = 20  # cm\n",
        "height = 5   # cm\n",
        "\n",
        "# 台形の面積を計算する\n",
        "area = (top_edge + bottom_edge) * height / 2\n",
        "\n",
        "# 結果の出力\n",
        "print(f\"{area}cm²\")\n"
      ],
